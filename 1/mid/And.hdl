/**
 * And gate:
 * if (a and b) out = 1, else out = 0 
 */
CHIP And {
    IN a, b;
    OUT out;
    
    PARTS:
   // 1. 先計算 Nand
    Nand(a=a, b=b, out=nandResult);

    // 2. 將 Nand 的結果反轉 (Not)，就變成了 And
    Not(in=nandResult, out=out);

}
