/**
 * Not gate:
 * if (in) out = 0, else out = 1
 */
CHIP Not {
    IN in;
    OUT out;

    PARTS:
   // 將輸入訊號 in 同時接到 Nand 的 a 和 b
    Nand(a=in, b=in, out=out);
}

